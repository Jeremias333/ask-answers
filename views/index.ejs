<%- include("partials/header.ejs") %>
<body>
	<%- include("partials/navbar.ejs") %>
	<div class="container">
		<h1><a href="/ask" style="color: white">ASK now...</a></h1>
		<% asks.forEach(ask => { %>
		<div class="card">
			<div class="card-body">
				<h3><%= ask.title %></h3>
			</div>
			<div class="card-footer">
				<button class="btn btn-primary">Answer</button>
			</div>
		<div/>
		<% }) %>
	</div>
</body>
<%- include("partials/footer.ejs") %>